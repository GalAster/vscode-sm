{
    "private": true,
    "name": "vscode-sm",
    "displayName": "Simple Math Support",
    "description": "Highlight and formatter for Simple Math",
    "publisher": "aster",
    "version": "0.1.0",
    "icon": "logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/GalAster/vscode-sm.git"
    },
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "engines": {
        "vscode": "^1.39.0"
    },
    "activationEvents": [],
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install && tsc",
        "build": "yarn lint && ts-node syntax/build.ts && tsc",
        "pack": "yarn build && vsce package",
        "lint": "tslint **/*.ts --fix",
        "reset": "git reset Head~ --soft"
    },
    "contributes": {
        "languages": [
            {
                "id": "sm",
                "aliases": [
                    "Simple Math"
                ],
                "extensions": [
                    ".sm",
                    ".simplemath"
                ],
                "filenames": [],
                "filenamePatterns": [],
                "mimetypes": [],
                "configuration": "./syntax/sm.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sm",
                "scopeName": "source.sm",
                "path": "./syntax/sm.tmLanguage.json"
            },
            {
                "scopeName": "markdown.sm.codeblock",
                "path": "./syntax/sm.markdown.json",
                "injectTo": [
                    "text.html.markdown"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block.sm": "sm"
                }
            }
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^13.7.6",
        "vscode": "^1.1.36"
    },
    "__metadata": {
        "id": "77108132-b3a9-4456-b8e6-05fca177371b",
        "publisherDisplayName": "Aster",
        "publisherId": "3406b78c-f287-4619-8d82-7c97998693e3"
    }
}
